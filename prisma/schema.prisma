generator client {
  provider      = "prisma-client-js"
  engineType    = "library"
  binaryTargets = ["native", "windows"]

  // TODO 注释这一行重新 generate
  output = "../release/app/node_modules/@prisma/client"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// TODO 自定义自己的model
model Information {
  id     Int    @id @default(autoincrement())
  label  String
  link   String
  detail String
  finish String
}
